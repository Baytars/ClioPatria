<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
    <!ENTITY dc 'http://purl.org/dc/elements/1.1/'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY qt 'http://www.w3.org/2001/sw/DataAccess/tests/test-query#'>
]>

<rdf:RDF
    xmlns:dc="&dc;"
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:qt="&qt;"
>
<rdf:Description rdf:about="&qt;"
    dc:creator="Andy Seaborne"
    dc:date="2004-07"
    dc:description="Query test case vocabulary"
    dc:format="RDF"
    dc:publisher="W3C RDF Data Access Working Group"
    dc:subject=""
    dc:title="Query test case vocabulary"
    rdfs:comment="Vocabulary for query test cases">
  <dc:identifier rdf:resource="&qt;"/>
</rdf:Description>

<rdfs:Class rdf:about="&qt;QueryTest"
    rdfs:comment="The class of query tests"/>

<rdf:Property rdf:about="&qt;query"
    rdfs:comment="The query to ask">
  <rdfs:domain rdf:resource="&qt;QueryTest"/>
</rdf:Property>

<rdf:Property rdf:about="&qt;data"
    rdfs:comment="Optional: data for the query test">
  <rdfs:domain rdf:resource="&qt;QueryTest"/>
  <rdfs:range rdf:resource="&rdfs;Resource"/>
</rdf:Property>

<rdf:Property rdf:about="&qt;graphData">
  <rdfs:comment>Optional: named-graph only data for the query test (ie. not loaded into the background graph)</rdfs:comment>
  <rdfs:domain rdf:resource="&qt;QueryTest"/>
  <rdfs:range rdf:resource="&rdfs;Resource"/>
</rdf:Property>

</rdf:RDF>
