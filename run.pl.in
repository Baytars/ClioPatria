:- load_files([ cliopatria('applications/help/load'),
		library(http/http_log),
		library(semweb/rdf_portray),
		cliopatria(cliopatria)
	      ],
	      [ silent(true),
		if(changed)
	      ]).

:- debug(http(request)).

