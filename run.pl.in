:- load_files([ library(semweb/rdf_db)
	      ],
	      [ silent(true),
		if(changed)
	      ]).

:- rdf_register_ns(cornetto, 'http://purl.org/vocabularies/cornetto/').

file_search_path(cliopatria,    '@CLIOPATRIA@').
